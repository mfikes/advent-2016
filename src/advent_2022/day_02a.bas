  5 T0=TIME
 10 T=0
 20 N=2500
 30 F$="AOC02I"
 40 ASSIGN# 1 TO F$
 50 FOR I=1 TO N
 60 READ# 1 ; X$
 70 L=NUM(X$)-64
 80 R=NUM(X$[3])-87
 90 IF L=R THEN S=3 @ GOTO 110
100 IF L=3 AND R=1 OR L=2 AND R=3 OR L=1 AND R=2 THEN S=6 ELSE S=0
110 T=T+S+R
120 NEXT I
130 ASSIGN# 1 TO *
140 DISP T
145 DISP TIME-T0;"s"
150 END
