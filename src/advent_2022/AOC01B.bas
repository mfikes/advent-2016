 10 T0=TIME
 20 N=2244
 30 F$="AOC01I"
 40 ASSIGN# 1 TO F$
 50 S=0
 60 M0=0 @ M1=0 @ M2=0
 70 FOR I=1 TO N
 80 READ# 1 ; X
 90 S=S+X
100 IF X<>0 THEN GOTO 160
110 IF S<M0 THEN GOTO 150
120 IF S<M1 THEN M0=S @ GOTO 150
130 IF S<M2 THEN M0=M1 @ M1=S @ GOTO 150
140 M0=M1 @ M1=M2 @ M2=S
150 S=0
160 NEXT I
170 ASSIGN# 1 TO *
180 DISP "Result";M0+M1+M2
190 DISP TIME-T0;"s"
200 END
